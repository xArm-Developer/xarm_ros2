    <link name="${prefix}link_eef" />

    <joint
      name="${prefix}joint_eef"
      type="fixed">
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <parent
        link="${prefix}link6" />
      <child
        link="${prefix}link_eef" />
    </joint>
  </xacro:macro>
</robot>