      <dynamics damping="1.0" friction="1.0"/>
    </joint>
    <link name="${prefix}link6">
      <inertial>
        <origin
          xyz="0.0 -0.00194 -0.0102"
          rpy="0 0 0" />
        <mass
          value="0.180" />
        <inertia
          ixx="7.726E-05"
